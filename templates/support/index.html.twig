{% extends 'layoutadmin.html.twig' %}

{% block title %}Lucette{% endblock %}

{% block content %}
<div class="breadcrumbs">
    <div class="col-sm-4">
        <div class="page-header float-left">

        </div>
    </div>
    <div class="col-sm-8">
        <div class="page-header float-right">
            <div class="page-title">
                <ol class="breadcrumb text-right">
                    <li><a href="{{ path("cms_dashboard") }}">Dashboard</a></li>
                    <li class="active">Support</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title">Support</strong>
                    </div>
                    <div class="card-body">
                        <div class="tile col-lg-4" >



                            {{ form_start(form) }}
                            <div class="form-group">
                                {{ form_errors(form.object) }}
                                {{ form_label(form.object) }}
                                {{ form_widget(form.object, {'attr':{'class': 'form-control' }}) }}
                            </div>
                            <div class="form-group">
                                {{ form_errors(form.service) }}
                                {{ form_label(form.service) }}
                                {{ form_widget(form.service, {'attr':{'class': 'form-control' }}) }}
                            </div>
                            <div class="form-group">
                                {{ form_errors(form.Message) }}
                                {{ form_label(form.Message) }}
                                {{ form_widget(form.Message, {'attr':{'class': 'form-control' }}) }}
                            </div>




                            <button type="submit" class="btn btn-info">Send</button>
                            <a href="{{ path("cms_dashboard") }}"  class="btn btn-danger">Return</a>

                            {{ form_end(form) }}

                        </div>
                        <script src="{{asset('vendors/jquery/dist/jquery.min.js')}}"></script>
                      	<script src="{{asset('vendors/popper.js/dist/umd/popper.min.js')}}"></script>
                      	<script src="{{asset('vendors/bootstrap/dist/js/bootstrap.min.js')}}"></script>
                      	<script src="{{asset('assets/js/main.js')}}"></script>
                        {% endblock %}
