{# templates/security/login.html.twig #}
{# ... you will probably extend your base template, like base.html.twig #}
{% extends 'baselogin.html.twig' %}

{% block body %}


    <div class="sufee-login d-flex align-content-center flex-wrap">
        <div class="container">
            <div class="login-content">
                <div class="login-logo">
                    <a href="https://lucette.market/">
                        <img class="align-content" style="70%;"  src="{{asset('images/lucette.png')}}" alt="Lucette">
                    </a>
                </div>
                <div class="login-form">


                    <div >
                        <div class="card bg-primary">
                            <div class="card-body">
                                <blockquote class="blockquote mb-0 text-light">
                                    <p class="text-light">
                                        Dear {{ user.username }}, your account has been successfully saved. Thank you for your registration!</p>
                                    <footer class="blockquote-footer text-light"><cite title="Source Title">Lucette Market Team</cite></footer>
                                </blockquote>
                            </div>
                        </div>
                    </div>




                    <div class="register-link m-t-15 text-center">
                        <p><a href="{{ path('login') }}">Login</a></p>
                    </div>




                </div>
            </div>
        </div>
    </div>


    <script src="{{asset('vendors/jquery/dist/jquery.min.js')}}"></script>
  	<script src="{{asset('vendors/popper.js/dist/umd/popper.min.js')}}"></script>
  	<script src="{{asset('vendors/bootstrap/dist/js/bootstrap.min.js')}}"></script>
  	<script src="{{asset('assets/js/main.js')}}"></script>


{% endblock %}
